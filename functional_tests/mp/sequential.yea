id: 0.mp.1-sequential
plugin:
  - wandb
assert:
  - :wandb:runs_len: 2
  - :wandb:runs[0][config]: {}
  - :wandb:runs[0][summary]:
      r1a: 1
      r2a: 2
  - :wandb:runs[0][exitcode]: 0
  - :op:contains:
      - :wandb:runs[0][output][stdout]
      - another line 1st runcontinue onecontinue one again
  - :op:contains:
      - :wandb:runs[0][output][stderr]
      - extra error the one
  - :wandb:runs[1][config]: {}
  - :wandb:runs[1][summary]:
      r1a: 11
      r2b: 22
  - :wandb:runs[1][exitcode]: 0
  - :op:contains:
      - :wandb:runs[1][output][stdout]
      - another line 2nd runcontinue twocontinue two again
  - :op:contains:
      - :wandb:runs[1][output][stderr]
      - extra error the two
